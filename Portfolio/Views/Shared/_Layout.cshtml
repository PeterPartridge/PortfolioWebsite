<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/themes/redmond/jquery-ui.redmond.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/themes/base/base.css" rel="stylesheet" /> 
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/index.scss" rel="stylesheet" />
   
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        @*<hr class="hiddenAtStart" />
        <footer class="hiddenAtStart">
            <p>&copy; @DateTime.Now.Year - WebPartridge</p>
        </footer>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
